#!/bin/sh
killall vw
# An example cluster command.  This just runs on one machine.
export FLAGS='-l 20 --power_t 0.5 --daemon --predictto localhost:39525'
./vw -l 20 --delayed_global --power_t 0.5 --multisource 2 --port 39525 --quiet &
sleep 1
./vw --delayed_global --port 39523 $FLAGS --quiet &
./vw --delayed_global --port 39524 $FLAGS --quiet &
./vw -d data --sendto localhost:39523 --sendto localhost:39524 
